public class LCM {
    public static void main(String[] args) {
        int a = 15, b = 20, max = Math.max(a, b);
        while (true) {
            if (max % a == 0 && max % b == 0) {
                System.out.println("LCM is: " + max);
                break;
            }
            max++;
        }
    }
}
